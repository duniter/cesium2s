{
  "name": "cesium",
  "short_name": "Cesium²",
  "description": "Manage G1 wallet",
  "author": "Benoit Lavenier <benoit.lavenier@e-is.pro>",
  "homepage_url": "https://cesium.app",
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Cesium²",
    "default_icon": {
      "32": "img/logo_32px.png",
      "60": "img/logo_60px.png",
      "96": "img/logo_96px.png",
      "128": "img/logo_128px.png",
      "192": "img/logo_192px.png"
    }
  },
  "icons": {
    "32": "img/logo_32px.png",
    "60": "img/logo_60px.png",
    "96": "img/logo_96px.png",
    "128": "img/logo_128px.png",
    "192": "img/logo_192px.png"
  },
  "manifest_version": 2,
  "protocol_handlers": [
    {
      "protocol": "web+june",
      "name": "web-june",
      "uriTemplate": "/home?uri=%s"
    }
  ]
}
